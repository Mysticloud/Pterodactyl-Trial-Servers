{
  "name": "pterodactyl-trial-servers",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3500",
    "preview": "vite preview --port 3500",
    "serve": "node --watch server.js",
    "serve-run": "node server.js",
    "dev-start": "npm-run-all --parallel dev serve",
    "start": "npm-run-all --parallel preview serve-run",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.0",
    "mongodb": "^6.8.0",
    "node-cron": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.24.1",
    "react-use-websocket": "^4.8.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "npm-run-all": "^4.1.5",
    "vite": "^5.3.1"
  }
}
